    "@angular/cli": "^17.0.0",
    "@angular/compiler-cli": "^17.0.0",
    "@angular/language-service": "^17.0.0",
    "@nx/angular": "^17.0.0",
    "@nx/cypress": "^17.0.0",
    "@nx/eslint-plugin": "^17.0.0",
    "@nx/jest": "^17.0.0",
    "@nx/js": "^17.0.0",
    "@nx/linter": "^17.0.0",
    "@nx/workspace": "^17.0.0",
    "@schematics/angular": "^17.0.0",
    "@types/jest": "^29.4.0",
    "@types/node": "^18.14.2",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "concurrently": "^8.2.0",
    "cypress": "^13.0.0",
    "eslint": "^8.48.0",
    "eslint-config-prettier": "^9.0.0",
    "jest": "^29.4.1",
